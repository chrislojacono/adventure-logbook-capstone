{
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "extends": ["airbnb-base", "react-app"],
    "globals": {
      "document": true,
      "window": true,
      "$": true,
      "XMLHttpRequest": true,
      "allowTemplateLiterals": true
    },
    "rules": {
      "no-console": [1, { "allow": ["error", "warn"] }],
      "no-debugger": 1,
      "import/no-anonymous-default-export": "off",
      "class-methods-use-this": 0,
      "linebreak-style": 0,
      "max-len": [1,800,2]
    }
  }